{
  "manifest_version": 2,
  "name": "Cut fbclid",
  "version": "1.2.0.0",
  "description": "シェアされたページのURLからfbclidをカットします。",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "icon19.png"
    }
  },
  "background": {
    "scripts": ["scripts/background.js"]
  },
  "permissions":[
    "activeTab",
    "https://www.facebook.com/*"
  ]
}

